@model CloudCompare.Web.Models.SearchResultModel

@*home display TEST template
*@

    <link href="/Content/overflow-text-ellipsis.css" rel="stylesheet" type="text/css" />    

<div>
    <table id="dataTable" style="width:100%;">
        <tr>
            <td style="width:30%"><img src="@Model.VendorLogoURL" /></td>
            <td style="width:30%">@Model.ApplicationCostPerMonth</td>
            <td style="width:30%">@Model.ApplicationCostPerAnnum</td>
            <td style="width:30%">@Model.SetupFee</td>
            <td style="width:30%">@Model.FreeTrialPeriod</td>
        </tr> 
        <tr>
            <td style="width:70%">@Model.ServiceName</td>
        </tr>
        <tr>
            <td style="width:70%" colspan="4">
                <div class="ellipsify">
                    <div class="pre-dots"></div>
                    <div class="dots">&hellip;</div>
                    @Model.Description
                    <span class="hidedots1"></span>
                    <div class="hidedots2"></div>
                </div>
            </td>
            <td>
                <table>
                    <tr>
                        <td>
                            
                            <input type="submit" name=@Html.Encode("PROCEED_BUTTON_APPLICATION_ID_" + Model.CloudApplicationID.ToString()) value="More Info &gt;" />
                         </td>
                    </tr>
                    <tr>
                        <td>
                            <input type="submit" name="searchresultbutton" value="Proceed &gt;" />
                        </td>
                    </tr>
                </table>
                
            </td>
        </tr>
    </table>
</div>

                